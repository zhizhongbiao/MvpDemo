<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="LazyViewPager">
        <attr name="init_lazy_item_offset" format="float" />
        <attr name="init_lazy_scrollable" format="boolean" />

    </declare-styleable>

    <declare-styleable name="NestedRefreshLayout">
        <attr name="pullView" format="integer" />
        <attr name="pullViewHeight" format="dimension|reference" />
        <attr name="pullMinViewHeight" format="dimension|reference" />
        <attr name="pullMaxHeight" format="dimension|reference" />
        <attr name="refreshHeight" format="dimension|reference" />
        <attr name="animPlayDuration" format="integer" />
    </declare-styleable>

    <declare-styleable name="AdvancedPagerSlidingTabStrip">
        <attr name="apTabIndicatorColor" format="color" />
        <attr name="apTabUnderlineColor" format="color" />
        <attr name="apTabDividerColor" format="color" />
        <attr name="apTabTextSelectColor" format="color" />
        <attr name="apTabIndicatorHeight" format="dimension" />
        <attr name="apTabUnderlineHeight" format="dimension" />
        <attr name="apTabDividerPadding" format="dimension" />
        <attr name="apTabPaddingLeftRight" format="dimension" />
        <attr name="apTabPaddingTopBottom" format="dimension" />
        <attr name="apTabScrollOffset" format="dimension" />
        <attr name="apTabBackground" format="reference" />
        <attr name="apTabShouldExpand" format="boolean" />
        <attr name="apTabTextAllCaps" format="boolean" />
        <attr name="apTabDrawMode" format="enum">
            <enum name="normal" value="0" />
            <enum name="text" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="JPTabBar">
        <attr name="TabNormalColor" format="reference|color" />
        <attr name="TabSelectColor" format="reference|color" />
        <attr name="TabSelectBg" format="reference|color" />
        <attr name="TabTextSize" format="dimension|reference" />
        <attr name="TabIconSize" format="dimension|reference" />
        <attr name="TabIconFilter" format="boolean" />
        <attr name="TabMargin" format="dimension|reference" />
        <attr name="TabAnimate" format="enum">
            <enum name="Flip" value="1" />
            <enum name="Rotate" value="2" />
            <enum name="Scale" value="3" />
            <enum name="Jump" value="4" />
            <enum name="None" value="5" />
        </attr>
        <attr name="TabMiddleBottomDis" format="dimension|reference" />
        <attr name="TabMiddleView" format="reference" />
        <attr name="TabMiddleHMargin" format="dimension|reference" />
        <attr name="BadgeColor" format="reference|color" />
        <attr name="BadgePadding" format="dimension|reference" />
        <attr name="BadgeTextSize" format="reference|dimension" />
        <attr name="BadgeVerticalMargin" format="reference|dimension" />
        <attr name="BadgeHorizonalMargin" format="reference|dimension" />
    </declare-styleable>

    <declare-styleable name="StarBar">
        <!--星星间距-->
        <attr name="starDistance" format="dimension" />
        <!--星星大小-->
        <attr name="starSize" format="dimension" />
        <!--星星个数-->
        <attr name="starCount" format="integer" />
        <!--星星空图-->
        <attr name="starEmpty" format="reference" />
        <!--星星满图-->
        <attr name="starFill" format="reference" />
    </declare-styleable>

    <!--自定义圆角ImageView -->
    <!--TODO WaterFlower comment this line-->
    <!--<declare-styleable name="CircleImageView">-->
        <!--<attr name="circle" format="boolean" />-->


        <!--&lt;!&ndash;<attr name="radius" format="dimension" />&ndash;&gt;-->
    <!--</declare-styleable>-->

    <attr name="type">
        <enum name="none" value="0" />
        <enum name="circle" value="1" />
        <enum name="rounded_rect" value="2" />
    </attr>
    <attr name="borderColor" format="color" />
    <attr name="borderWidth" format="dimension" />
    <attr name="rectRoundRadius" format="dimension" />

    <declare-styleable name="ImageViewPlus">
        <attr name="type" />
        <attr name="borderColor" />
        <attr name="borderWidth" />
        <attr name="rectRoundRadius" />
    </declare-styleable>


    <declare-styleable name="PasswordInputView">
        <attr name="pivBorderColor" format="color" />
        <attr name="pivBorderWidth" format="dimension" />
        <attr name="pivBorderRadius" format="dimension" />
        <attr name="pivPasswordColor" format="color" />
        <attr name="pivPasswordWidth" format="dimension" />
        <attr name="pivPasswordRadius" format="dimension" />
        <attr name="pivPasswordLength" format="integer" />
    </declare-styleable>

    <declare-styleable name="DiscView">
        <!--圆圈线的宽度-->
        <attr name="dvStrokenWidth" format="dimension" />
        <!--圆圈颜色-->
        <attr name="dvStrokenColor" format="color" />
        <!--外装饰圆是否显示-->
        <attr name="dvOuterCircleShow" format="boolean" />
        <!--外装饰圆距离圆环边距-->
        <attr name="dvOuterCirclePad" format="dimension" />

        <!--内部装饰圆是否显示-->
        <attr name="dvInnerCircleShow" format="boolean" />
        <!--内部装饰圆距离圆环边距-->
        <attr name="dvInnerCirclePad" format="dimension" />

        <!--底部圆形是否显示-->
        <attr name="dvBottomCircleIsShow" format="boolean" />

        <!--圆形指示图片-->
        <attr name="dvIndicatorDraw" format="reference" />

        <!--圆盘开始旋转角度值-->
        <attr name="dvStartRotateAngle" format="integer" />
        <!--圆盘可旋转角度范围-->
        <attr name="dvAngleRotateSpan" format="integer" />

        <!--圆盘模式-->
        <attr name="dvCircleMode">
            <enum name="normal" value="1" />
            <enum name="round" value="2" />
            <enum name="dot" value="3" />
        </attr>
    </declare-styleable>


    <declare-styleable name="PayPsdInputView">

        <attr name="maxCount" format="integer" />
        <attr name="circleColor" format="color" />
        <attr name="bottomLineColor" format="color" />
        <attr name="radius" format="dimension" />
        <attr name="divideLineWidth" format="dimension" />
        <attr name="divideLineColor" format="color" />
        <attr name="rectAngle" format="dimension" />
        <attr name="focusedColor" format="color" />
        <attr name="psdType" format="enum">
            <enum name="weChat" value="0" />
            <enum name="bottomLine" value="1" />
        </attr>
    </declare-styleable>





    <declare-styleable name="CircleImageView">
        <attr name="civ_border_width" format="dimension" />
        <attr name="civ_border_color" format="color" />
        <attr name="civ_border_overlay" format="boolean" />
        <attr name="civ_fill_color" format="color" />
    </declare-styleable>


</resources>